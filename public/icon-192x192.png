<svg width="192" height="192" viewBox="0 0 192 192" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background circle with gradient -->
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1976d2;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1565c0;stop-opacity:1" />
    </linearGradient>
    <filter id="shadow" x="-50%" y="-50%" width="200%" height="200%">
      <dropShadow dx="2" dy="2" stdDeviation="3" flood-color="#000000" flood-opacity="0.3"/>
    </filter>
  </defs>
  
  <circle cx="96" cy="96" r="96" fill="url(#bgGradient)"/>
  
  <!-- Main gear -->
  <g transform="translate(96, 96)">
    <!-- Gear teeth -->
    <path d="M-6,-48 L6,-48 L6,-36 L18,-36 L18,-18 L36,-18 L36,-6 L48,-6 L48,6 L36,6 L36,18 L18,18 L18,36 L6,36 L6,48 L-6,48 L-6,36 L-18,36 L-18,18 L-36,18 L-36,6 L-48,6 L-48,-6 L-36,-6 L-36,-18 L-18,-18 L-18,-36 L-6,-36 Z" fill="#ffffff" filter="url(#shadow)"/>
    <!-- Inner circle -->
    <circle cx="0" cy="0" r="18" fill="#1976d2"/>
    <!-- Center hole -->
    <circle cx="0" cy="0" r="9" fill="#ffffff"/>
    <!-- Center detail -->
    <circle cx="0" cy="0" r="4" fill="#1976d2"/>
  </g>
  
  <!-- Large spanner -->
  <g transform="translate(48, 48) rotate(-30)">
    <rect x="-3" y="-24" width="6" height="48" fill="#ffffff" opacity="0.9" filter="url(#shadow)"/>
    <circle cx="0" cy="-18" r="6" fill="#ffffff" opacity="0.9"/>
    <circle cx="0" cy="18" r="6" fill="#ffffff" opacity="0.9"/>
    <!-- Spanner jaw detail -->
    <rect x="-2" y="-20" width="4" height="4" fill="#1976d2"/>
    <rect x="-2" y="16" width="4" height="4" fill="#1976d2"/>
  </g>
  
  <!-- Small gear -->
  <g transform="translate(144, 48)">
    <!-- Mini gear teeth -->
    <path d="M-3,-18 L3,-18 L3,-12 L9,-12 L9,-6 L18,-6 L18,-3 L12,-3 L12,3 L9,3 L9,9 L3,9 L3,18 L-3,18 L-3,12 L-9,12 L-9,6 L-12,6 L-12,3 L-18,3 L-18,-3 L-9,-3 L-9,-6 L-3,-6 Z" fill="#ffffff" opacity="0.8"/>
    <!-- Inner circle -->
    <circle cx="0" cy="0" r="7" fill="#1976d2"/>
    <circle cx="0" cy="0" r="3" fill="#ffffff"/>
  </g>
  
  <!-- Small wrench -->
  <g transform="translate(144, 144) rotate(45)">
    <rect x="-1.5" y="-12" width="3" height="24" fill="#ffffff" opacity="0.7"/>
    <circle cx="0" cy="-9" r="3" fill="#ffffff" opacity="0.7"/>
    <circle cx="0" cy="9" r="3" fill="#ffffff" opacity="0.7"/>
  </g>
</svg>
